;; ~/.aws/config

[default]
region = ap-northeast-1
output = json

[profile SAMPLE_PROFILE]
region = ap-northeast-1
output = json
sso_account_id = 123456789012
sso_role_name = permission-set-name-for-sample-profile

[sso-session AWS_SESSION]
sso_start_url = https://x-xxxxxxxxx.awsapps.com/start
sso_region = ap-northeast-1
output = json

[profile XXX_PRD]
